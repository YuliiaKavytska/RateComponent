    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "sass": "^1.44.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start lint stylelint",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
